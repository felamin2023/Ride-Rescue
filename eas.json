{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" }
    },
    "preview": {
      "environment": "preview",
      "android": { "buildType": "apk" }
    },
    "preview2": {
      "environment": "preview",
      "android": { "gradleCommand": ":app:assembleRelease" }
    },
    "preview3": {
      "environment": "development",
      "developmentClient": true
    },
    "preview4": {
      "environment": "preview",
      "distribution": "internal"
    },
    "production": {
      "environment": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
